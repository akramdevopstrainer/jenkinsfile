pipeline {
    agent any

    stages {
        stage('Clone code from Git') {
            steps {
                echo 'Code Clone success'
            }
        }
        stage('Build Code') {
            steps {
                echo 'Code built successfully'
            }
        }
        stage('Code Package') {
            steps {
                echo 'Code Packaged'
            }
        }
        stage('Test Code') {
            steps {
                echo 'Testing success'
            }
        }
        stage('Deploy Code') {
            steps {
                echo 'Code deploy success'
            }
        }
    }
}
